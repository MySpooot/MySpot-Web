{
    "name": "myspot",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "cross-env REACT_APP_ENV=local craco start",
        "start:dev": "cross-env REACT_APP_ENV=dev craco start",
        "build": "REACT_APP_ENV=production GENERATE_SOURCEMAP=false craco build",
        "eject": "craco eject",
        "lint": "eslint --ext ts,tsx ./",
        "lint:fix": "eslint --ext ts,tsx ./ --fix",
        "stylelint": "stylelint \"./src/**/*.{ts,tsx}\"",
        "stylelint:fix": "stylelint \"./src/**/*.{ts,tsx}\" --fix",
        "prettier": "prettier -w ./src",
        "e2e": "cypress run",
        "e2e:open": "cypress open",
        "deploy": "deploy-aws-s3-cloudfront --bucket myspot.co.kr --source ./build --delete true --distribution E1DAGMAK50L60D --invalidation-path '/*'"
    },
    "dependencies": {
        "@emotion/react": "^11.5.0",
        "@emotion/styled": "^11.3.0",
        "@rodmg/react-div-100vh": "^0.3.5",
        "axios": "^0.24.0",
        "dayjs": "^1.10.7",
        "jotai": "^1.6.0",
        "query-string": "^7.1.1",
        "react": "^17.0.2",
        "react-copy-to-clipboard": "^5.0.4",
        "react-dom": "^17.0.2",
        "react-intersection-observer": "^8.33.1",
        "react-kakao-maps-sdk": "^1.0.4",
        "react-query": "^3.34.11",
        "react-query-helper": "^0.1.0",
        "react-responsive": "^9.0.0-beta.4",
        "react-router": "^6.2.1",
        "react-router-dom": "^6.2.1",
        "react-scripts": "4.0.3",
        "reactjs-popup": "^2.0.5",
        "regenerator-runtime": "^0.13.9",
        "src": "link:./src"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
        "@babel/preset-react": "^7.16.7",
        "@craco/craco": "^6.4.2",
        "@emotion/babel-plugin": "^11.7.2",
        "@emotion/babel-preset-css-prop": "^11.2.0",
        "@stylelint/postcss-css-in-js": "^0.37.2",
        "@swc/helpers": "^0.3.2",
        "@testing-library/cypress": "^8.0.2",
        "@types/node": "^12.0.0",
        "@types/react": "^17.0.0",
        "@types/react-copy-to-clipboard": "^5",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "aws-sdk": "^2.1100.0",
        "craco-swc": "^0.2.0",
        "cross-env": "^7.0.3",
        "cypress": "^9.5.2",
        "deploy-aws-s3-cloudfront": "^3.6.0",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "postcss-safe-parser": "4.0.2",
        "prettier": "^2.4.1",
        "react-error-overlay": "6.0.9",
        "stylelint": "^13.13.1",
        "stylelint-config-idiomatic-order": "^8.1.0",
        "stylelint-config-recommended": "^6.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-order": "^5.0.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.5.4"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "packageManager": "yarn@3.1.1"
}
